   "source": [
    "def textispalindrome(x):\n",
    "    lastIndex = len(x)-1\n",
    "    line = len(x)//2\n",
    "    for i in range(0, line):\n",
    "        if x[i] != x[lastIndex-i]:\n",
    "            return False\n",
    "    return True"
   ]
